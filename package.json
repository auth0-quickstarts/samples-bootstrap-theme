{
  "name": "samples-bootstrap-theme",
  "version": "1.0.0",
  "description": "Common assets and styles for the Auth0 Quickstart applications, based on Bootstrap",
  "scripts": {
    "build": "gulp",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "publish:cdn": "ccu --trace"
  },
  "files": [
    "dist"
  ],
  "main": "dist/css/auth0-theme.min.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0-samples/samples-bootstrap-theme.git"
  },
  "keywords": [
    "auth0",
    "css",
    "bootstrap4",
    "scss",
    "node"
  ],
  "author": "Auth0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/auth0-samples/samples-bootstrap-theme/issues"
  },
  "homepage": "https://github.com/auth0-samples/samples-bootstrap-theme#readme",
  "private": true,
  "devDependencies": {
    "@auth0/component-cdn-uploader": "github:auth0/component-cdn-uploader#v2.2.2",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "dependencies": {},
  "engines": {
    "node": ">= 7.6"
  },
  "ccu": {
    "name": "auth0-samples-theme",
    "cdn": "https://cdn.auth0.com",
    "mainBundleFile": "auth0-theme.min.css",
    "bucket": "assets.us.auth0.com",
    "localPath": "dist",
    "digest": {
      "hashes": [
        "sha384"
      ],
      "extensions": [
        ".css"
      ]
    }
  }
}
